<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myapplication.HomeFragment"
        android:label="Inicio"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_likes"
        android:name="com.example.myapplication.LikesFragment"
        android:label="Likes"
        tools:layout="@layout/fragment_likes" />

    <fragment
        android:id="@+id/navigation_mensajes"
        android:name="com.example.myapplication.MensajesFragment"
        android:label="Mensajes"
        tools:layout="@layout/fragment_mensajes" />

    <fragment
        android:id="@+id/navigation_perfil"
        android:name="com.example.myapplication.ProfileFragment"
        android:label="Perfil"
        tools:layout="@layout/activity_profile" />

</navigation>